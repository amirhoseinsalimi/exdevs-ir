{
  "name": "expteam-ir",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "start": "gulp build && pm2 start ./bin/www --name expteam-ir",
    "restart": "gulp build && pm2 restart expteam-ir",
    "stop": "pm2 stop expteam-ir",
    "dev": "gulp",
    "monit": "pm2 monit",
    "db:migrate": "knex migrate:latest",
    "db:seeds": "knex seed:run",
    "db:seed": "npm run db:seeds",
    "db:rollback": "knex migrate:rollback --all",
    "test": "cross-env NODE_ENV=testing mocha --timeout 10000",
    "lint": "eslint --fix gulpfile.js app.js counter.js src/js/script.js"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
