    "express": "~4.16.0",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.2",
    "knex": "^0.20.15",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "pm2": "^3.5.2",
    "pug": "^2.0.4",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "serve-favicon": "^2.5.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
