{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": "airbnb",
  "globals": {
    "particlesJS" : true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-console": "warn",
    "no-shadow": "off",
    "no-unused-vars": "warn",
    "no-use-before-define": "off"
  }
}